{
    "extends": [
        "config:js-app",
        ":gitSignOff",
        ":rebaseStalePrs",
        "group:allNonMajor",
        "group:linters",
        "group:test"
    ],
    "labels": [
        "kind/dependency upgrade"
    ],
    "git-submodules": {
        "enabled": true
    },
    "customManagers": [
        {   "fileMatch": [".gitmodules"],

            "packageNameTemplate": "https://github.com/janus-idp/helm-backstage",
            "datasourceTemplate": "git-tags"
        }
    ]
}
